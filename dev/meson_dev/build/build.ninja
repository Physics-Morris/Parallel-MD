 COMMAND = /usr/bin/python3.6 /usr/bin/meson --internal dist /work/morris123/MD/dev/meson_dev /work/morris123/MD/dev/meson_dev/build /usr/bin/python3.6 /usr/bin/meson
 pool = console

build dist: phony meson-dist

# Suffix

build meson-scan-build: CUSTOM_COMMAND PHONY
 COMMAND = /usr/bin/python3.6 /usr/bin/meson --internal scanbuild /work/morris123/MD/dev/meson_dev /work/morris123/MD/dev/meson_dev/build /usr/bin/python3.6 /usr/bin/meson
